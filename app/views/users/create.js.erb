$(".add-user-form-content #error-container").html("");
<% if @user.errors.full_messages.present? %>
  $(".add-user-form-content #error-container").html("<%= j render "shared/errors", errors: @user.errors.full_messages %>");
<% else %>
  hideModel();
  getUsersList("<%= @user_type %>")
  showFlashMessage('<%= t('toastr.type.success') %>', '<%= t('toastr.message.successfully_created', type: 'User') %>');
<% end %>
